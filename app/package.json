{
  "name": "keipes-ai-electron",
  "version": "1.0.0",
  "description": "Keipes AI Electron Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "dist": "electron-builder --publish=never",
    "pack": "electron-builder --dir",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "keywords": [
    "electron",
    "ai",
    "desktop"
  ],
  "author": "Keipes",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.8",
    "electron": "^30.0.0",
    "electron-builder": "^25.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "axios": "^1.8.0"
  },
  "build": {
    "appId": "com.keipes.ai",
    "productName": "Keipes AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "src/",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
