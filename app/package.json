{
  "name": "keipes-ai-electron",
  "version": "1.0.0",
  "description": "Keipes AI Electron Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev:all": "npm run renderer:dev & npm start -- --dev",
    "renderer:dev": "vite --config vite.config.mjs",
    "renderer:build": "vite build --config vite.config.mjs",
    "build": "vite build --config vite.config.mjs && electron-builder",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "keywords": [
    "electron",
    "ai",
    "desktop"
  ],
  "author": "Keipes",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.8",
    "electron": "^30.0.0",
    "electron-builder": "^25.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.6.0",
    "axios": "^1.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^7.0.0"
  },
  "build": {
    "appId": "com.keipes.ai",
    "productName": "Keipes AI",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "src/",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
